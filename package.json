{
  "name": "autocrm",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:e2e:open": "cypress open",
    "seed-users:ci": "tsx scripts/seed-users-ci.ts",
    "supabase:env": "supabase status -o env > .env",
    "supabase:start": "supabase start",
    "test-supabase": "tsx scripts/test-supabase.ts",
    "seed:users": "tsx scripts/seed-users.ts",
    "cypress:open": "ELECTRON_ENABLE_LOGGING=1 start-server-and-test dev http://localhost:5173 'cypress open'",
    "cypress:run": "ELECTRON_ENABLE_LOGGING=1 start-server-and-test dev http://localhost:5173 'cypress run'",
    "cypress:component": "ELECTRON_ENABLE_LOGGING=1 cypress run --component",
    "cypress:e2e": "ELECTRON_ENABLE_LOGGING=1 start-server-and-test dev http://localhost:5173 'cypress run --e2e'",
    "test:e2e": "start-server-and-test dev http://localhost:5173 'cypress run'",
    "test:component": "ELECTRON_ENABLE_LOGGING=1 cypress run --component",
    "test:service-rep": "ELECTRON_ENABLE_LOGGING=1 cypress run --spec 'cypress/e2e/service-rep-access.cy.js'",
    "test:ai": "ELECTRON_ENABLE_LOGGING=1 start-server-and-test 'npm run dev:with-functions' http://localhost:5173 'cypress run --spec cypress/e2e/010-test-ai-function.cy.js'",
    "test:ai2": "ELECTRON_ENABLE_LOGGING=1 start-server-and-test 'npm run dev:with-functions' http://localhost:5173 'cypress run --spec cypress/e2e/011-process-ai-action.cy.js'",
    "test:ai3": "ELECTRON_ENABLE_LOGGING=1 start-server-and-test 'npm run dev:with-functions' http://localhost:5173 'cypress run --spec cypress/e2e/012-ai-assistant-history.cy.js'",
    "pretest": "npm run build",
    "test": "npm run test:component && npm run test:e2e",
    "seed:serve:local": "supabase functions serve seed-users --env-file ./supabase/functions/seed-users/.env.local",
    "seed:serve:prod": "supabase functions serve seed-users --env-file ./supabase/functions/seed-users/.env",
    "seed:deploy": "supabase functions deploy seed-users",
    "seed:run:local": "source .env && curl -i --location --request POST \"http://localhost:54321/functions/v1/seed-users\" --header \"Authorization: Bearer ${SEED_ADMIN_TOKEN}\" --header \"Content-Type: application/json\"",
    "seed:run:prod": "source .env && curl -i -L -X POST \"${SUPABASE_URL}/functions/v1/seed-users\" -H \"Authorization: Bearer ${SEED_ADMIN_TOKEN}\" -H \"Content-Type: application/json\"",
    "cypress:env": "tsx scripts/create-cypress-env.ts",
    "functions:serve": "supabase functions serve --env-file ./supabase/functions/test-ai-action/.env --no-verify-jwt",
    "dev:with-functions": "concurrently \"npm run dev\" \"npm run functions:serve\""
  },
  "dependencies": {
    "@aws-amplify/backend-graphql": "^0.3.2",
    "@aws-amplify/graphql-api-construct": "^1.18.6",
    "@aws-amplify/ui-react": "^6.5.5",
    "@eslint/js": "^9.18.0",
    "@hookform/resolvers": "^3.10.0",
    "@langchain/community": "^0.3.27",
    "@langchain/core": "^0.3.37",
    "@langchain/openai": "^0.4.2",
    "@radix-ui/react-alert-dialog": "^1.1.5",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.5",
    "@radix-ui/react-hover-card": "^1.1.5",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.47.15",
    "@tanstack/react-virtual": "^3.11.2",
    "aws-amplify": "^6.6.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "langchain": "^0.3.14",
    "langsmith": "^0.3.3",
    "lucide-react": "^0.471.1",
    "openai": "^4.79.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.54.2",
    "react-markdown": "^9.0.3",
    "react-router-dom": "^7.1.2",
    "remark-gfm": "^4.0.0",
    "tailwind-merge": "^2.6.0",
    "vitest": "^3.0.2",
    "zod": "^3.24.1",
    "zxcvbn": "^4.4.2"
